package kosta.order;

public class OrderMain {

	public static void main(String[] args) {

		String memberName = "홍길동";
		String memberAddress = "가산";
		String itemName = "갤럭시23";
		int itemPrice = 100;
		int orderQuantity = 10;
		int orderPrice = 1000;

		Order n1 = new Order(memberName, memberAddress, itemName, itemPrice, orderQuantity, orderPrice);

		n1.orderShow();

	}

}
